{
    "name": "wps\/secured",
    "description": "Provides secured for WordPress sites",
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "wp-secured.com",
            "email": "wp-secured@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "WPSEC_Vendor\\WPSEC\\": "classes\/",
            "WPSEC_Vendor\\Tests\\": "tests\/"
        },
        "classmap": [
            "classes\/"
        ]
    },
    "require": {
        "spomky-labs\/otphp": "^10",
        "mobiledetect\/mobiledetectlib": "^2.8",
        "scssphp\/scssphp": "^1.4",
        "tedivm\/jshrink": "~1.4",
        "endroid\/qr-code": "4.3.4"
    },
    "require-dev": {
        "phpunit\/phpunit": "^8",
        "bjeavons\/zxcvbn-php": "^1.0"
    }
}